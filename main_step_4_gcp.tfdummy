module "boundary_gcp_targets" {
  source = "./modules/boundary_gcp_targets"
  project_id = var.gcp_project_id
  gcp_region = var.gcp_region
  gke_username = var.gke_username
  gke_password = var.gke_password
  gke_num_nodes = 2
  prefix = var.prefix
}

output "gcp_kubernetes_cluster_name" {
  value = module.boundary_gcp_targets.kubernetes_cluster_name
}